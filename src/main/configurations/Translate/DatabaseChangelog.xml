<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="OpenZaak:1.1" author="Bibi">
        <comment>Add IDENTIFICATIE Table</comment>
        <createTable tableName="IDENTIFICATIE">
            <column name="ID" type="INTEGER" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" primaryKeyName="PK_IDENTIFICATIE"/>
            </column>
            <column name="IDENTIFICATIE" type="INTEGER"/>
            <column name="TYPE" type="java.sql.Types.VARCHAR(50)"/>
        </createTable>
    </changeSet> 

    <!-- See src/main/resources/springIbisTestToolCustom.xml -->
    <changeSet id="LadyBugCustom:1" author="Meric">
        <comment>Add SOAPAction Column</comment>
        <addColumn tableName="LADYBUG">
            <column name="SOAPACTION" type="VARCHAR(256)"/>
        </addColumn>
    </changeSet>

    <changeSet id="LadyBugCustom:2" author="Meric">
        <comment>Add Identificatie Column</comment>
        <addColumn tableName="LADYBUG">
            <column name="IDENTIFICATIE" type="VARCHAR(256)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>